package lv.raimonds;

public class LearnMethodsWithType {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		double area = getArea();
		
	}
	
	public static double getArea(){
		double num1 = 20.3;
		double num2 = 10.0;
		double result = num1 * num2;
		System.out.println("Area of rectangle: "+result);
		return result;
	}

}
