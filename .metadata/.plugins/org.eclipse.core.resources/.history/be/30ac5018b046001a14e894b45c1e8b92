package lv.raimonds;

import java.util.ArrayList;

public class TestTaskArray {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		ArrayList<String> cars = new ArrayList<>();
	
		cars.add("Volvo");
		cars.add("BMW");
		cars.add("Ford");
		cars.add("Mazda");
		
		for(String car : cars){
			System.out.println(car);
		}
		

	}

}
