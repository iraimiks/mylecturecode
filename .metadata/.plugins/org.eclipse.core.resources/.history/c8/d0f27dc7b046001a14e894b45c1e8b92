package lv.raimonds;

import java.util.ArrayList;

public class TaskTestArrayMethods {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		ArrayList<String> cars = new ArrayList<>();
		
		cars.add("Volvo");
		cars.add("BMW");
		cars.add("Ford");
		cars.add("Mazda");

		System.out.println();
		for(String car : cars){
			System.out.println(car);
		}
		System.out.println();
		cars.remove("Ford");
		for(String car : cars){
			System.out.println(car);
		}
		

	}

}
